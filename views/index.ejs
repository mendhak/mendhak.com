		<script type="text/javascript" src="/scripts/mendhak.js"></script>

	</head>
	


<body>


	
	<!--Time Bar-->
	<div id="progress-back" class="load-item">
		<div id="progress-bar"></div>
	</div>
	
	<!--Control Bar-->
	<div id="controls-wrapper" class="load-item">
		<div id="controls">
			
			<div id="navContainer"><a id="prevslide" class="load-item"></a><a id="play-button"><img id="pauseplay" src="images/pause_dull.png"/></a><a id="nextslide" class="load-item"></a></div>
		
			<!--Slide counter-->
			 <div id="slidecounter">
				&nbsp;<!--<span class="slidenumber"></span> / <span class="totalslides"></span>-->
			</div> 

			<!--Slide captions displayed here-->
			<div id="slidecaption"></div>
			
			<div id="badges">
				<a onclick="goTo('http://gpslogger.mendhak.com/');"><img src="/images/gpslogger.png" alt="GPSLogger" title="GPSLogger for Android" class="tooltip" /></a>
				<a onclick="goTo('http://mendhak.github.com');"><img src="/images/github.png" alt="Code Projects" title="Code Projects" class="tooltip" /></a>
				<a onclick="goTo('http://www.goodreads.com/user/show/6836662-mendhak');"><img src="/images/goodreads.png" alt="Goodreads" title="Goodreads Profile" class="tooltip" /></a>
				<a onclick="goTo('http://flickr.com/photos/mendhak/');"><img src="/images/fli.png" alt="Flickr photos" title="Flickr Photos" class="tooltip" /></a>
				<a onclick="goTo('https://instagram.com/mendhak/');"><img src="/images/inslogo.png" alt="insta" title="Instagram Photos" class="tooltip" /></a>

			</div>
		</div>
	</div>

</body>
</html>
